{
  "name": "voxassist-backend",
  "version": "1.0.0",
  "description": "VoxAssist Backend API - Express.js server for AI voice support",
  "main": "src/server.js",
  "scripts": {
    "start": "prisma generate --no-engine && prisma migrate deploy && node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test jest --testPathPattern=tests/unit --setupFilesAfterEnv=./tests/setup.js",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --setupFilesAfterEnv=./tests/setup.js",
    "test:all": "NODE_ENV=test jest --setupFilesAfterEnv=./tests/setup.js",
    "test:watch": "NODE_ENV=test jest --watch --setupFilesAfterEnv=./tests/setup.js",
    "test:coverage": "NODE_ENV=test jest --coverage --setupFilesAfterEnv=./tests/setup.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "node scripts/build-with-seeding.js",
    "build:simple": "prisma generate --no-engine && echo 'Backend build complete'",
    "test:load": "artillery run tests/load/load-test.yml",
    "db:generate": "prisma generate --no-engine",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node src/database/seed.js",
    "db:seed:production": "node scripts/database-seeder.js",
    "db:seed:staging": "node scripts/database-seeder.js --target-env=staging",
    "db:seed:dry-run": "node scripts/database-seeder.js --dry-run",
    "db:seed:test": "node scripts/test-seeding.js",
    "db:studio": "prisma studio",
    "deploy": "./scripts/deploy-with-seeding.sh",
    "deploy:render": "./scripts/deploy-with-seeding.sh --platform=render",
    "deploy:heroku": "./scripts/deploy-with-seeding.sh --platform=heroku",
    "deploy:docker": "./scripts/deploy-with-seeding.sh --platform=docker"
  },
  "dependencies": {
    "@google-cloud/speech": "^7.2.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "elevenlabs-node": "^2.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "openai": "^5.20.0",
    "plivo": "^4.75.1",
    "prisma": "^6.15.0",
    "puppeteer": "^24.19.0",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "twilio": "^5.9.0",
    "uuid": "^8.3.2",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "artillery": "^2.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.1.10",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "express",
    "api",
    "voice",
    "ai",
    "twilio",
    "gemini"
  ],
  "author": "Harsh Hirawat",
  "license": "MIT"
}
